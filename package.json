{
  "name": "zoas-store-client",
  "version": "1.0.0",
  "description": "A client for Zoas Store GraphQL API",
  "main": "index.js",
  "repository": "git@github.com:jeovazero/zoas-store-client.git",
  "author": "contato@jeova.ninja",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "prettier": "prettier-standard '**/*.{jsx,js}' && yarn lint:fix",
    "lint:fix": "standard --fix '**/*.{jsx,js}'"
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "prettier-standard",
      "standard --env jest --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "prettier-standard": "^9.1.1",
    "standard": "^12.0.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
